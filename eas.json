{
	"cli": {
		"version": ">= 9.0.10",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"autoIncrement": true,
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"EXPO_PUBLIC_APP_VARIANT": "dev",
				"EXPO_PUBLIC_API_URL": "https://difeapp.com/api",
				"EXPO_PUBLIC_WS_URL": "wss://difeapp.com/ws",
				"EXPO_PUBLIC_MOCK_LOGIN": "true",
				"EXPO_PUBLIC_MOCK_EMAIL": "test1@gmail.com",
				"EXPO_PUBLIC_MOCK_PASSWORD": "password1234!"
			},
			"channel": "development"
		},
		"preview": {
			"distribution": "internal"
		},
		"production": {
			"autoIncrement": true,
			"ios": {
				"image": "latest"
			},
			"env": {
				"EXPO_PUBLIC_API_URL": "https://difeapp.com/api",
				"EXPO_PUBLIC_WS_URL": "wss://difeapp.com/ws"
			},
			"channel": "production"
		},
		"staging": {
			"autoIncrement": true,
			"ios": {
				"image": "latest"
			},
			"env": {
				"EXPO_PUBLIC_APP_VARIANT": "staging",
				"EXPO_PUBLIC_API_URL": "https://difeapp.com/api",
				"EXPO_PUBLIC_WS_URL": "wss://difeapp.com/ws"
			},
			"channel": "staging"
		}
	},
	"submit": {
		"production": {
			"ios": {
				"appleId": "dife.teamdiverse@gmail.com",
				"ascAppId": "6502968541",
				"appleTeamId": "YV6HCWQ47U"
			}
		},
		"staging": {
			"ios": {
				"appleId": "dife.teamdiverse@gmail.com",
				"ascAppId": "6670191789",
				"appleTeamId": "YV6HCWQ47U"
			}
		}
	}
}
